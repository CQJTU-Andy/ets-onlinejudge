<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cqjtu.ets.account.mapper.AccountMapper">
    <resultMap id="BaseResultMap" type="com.cqjtu.ets.account.model.entity.AccountEntity">
        <id column="id" property="id"/>
        <result column="account_name" property="accountName"/>
        <result column="account_password" property="accountPassword"/>
        <result column="gender" property="gender"/>
    </resultMap>
    <select id="findAll" resultMap="BaseResultMap">
        select * from account order by id desc
    </select>
</mapper>
